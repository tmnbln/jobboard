{
    "manifest_version": 2,
    "name": "Job Board",
    "version": "1.0",
    "description": "An extension to manage job offers",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}